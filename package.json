{
	"name": "cuponz",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "cross-env GENERATE_SOURCEMAP=false npm run build:client && npm run build:server",
		"build:languages": "node scripts/generateSupportedLanguages.cjs",
		"build:client": "vite build --ssrManifest --outDir dist/client",
		"build:server": "vite build --ssr src/entry-server.jsx --outDir dist/server",
		"lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"build-dev": "npm run dev:client & npm run dev:server",
		"build-dev:client": "vite build --ssrManifest --outDir dist/client --watch",
		"build-dev:server": "vite build --ssr src/entry-server.jsx --outDir dist/server --watch",
		"test": "vitest run",
		"test-report": "vitest --run --reporter json --output-file TEST_REPORT.json",
		"format": "prettier . --write --ignore-unknown"
	},
	"lint-staged": {
		"*": "prettier . --write --ignore-unknown"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.56.2",
		"@testing-library/jest-dom": "^6.5.0",
		"@testing-library/react": "^16.0.1",
		"country-list": "^2.3.0",
		"date-fns": "^4.1.0",
		"framer-motion": "^11.11.1",
		"fuse.js": "^7.0.0",
		"husky": "^9.1.6",
		"jspdf": "^2.5.2",
		"jspdf-autotable": "^3.8.4",
		"libphonenumber-js": "^1.11.9",
		"prop-types": "^15.8.1",
		"react": "^18.3.1",
		"react-country-flag": "^3.1.0",
		"react-dom": "^18.3.1",
		"react-icons": "^5.3.0",
		"react-qr-code": "^2.0.15",
		"react-router-dom": "^6.26.2",
		"react-select": "^5.8.1",
		"sonner": "^1.5.0",
		"validator": "^13.12.0",
		"xlsx": "^0.18.5",
		"zustand": "^4.5.5"
	},
	"devDependencies": {
		"@types/react": "^18.3.9",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.1",
		"@vitejs/plugin-react-swc": "^3.7.0",
		"autoprefixer": "^10.4.20",
		"cross-env": "^7.0.3",
		"eslint": "^8.57.1",
		"eslint-plugin-react": "^7.36.1",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.12",
		"http-proxy": "^1.18.1",
		"jsdom": "^25.0.1",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"sharp": "^0.33.5",
		"svgo": "^3.3.2",
		"tailwindcss": "^3.4.13",
		"vite": "^5.4.7",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-image-optimizer": "^1.1.8",
		"vite-plugin-purge": "^1.0.3",
		"vite-plugin-tailwind-purgecss": "^0.3.3",
		"vite-plugin-webp-generator": "^0.0.2",
		"vitest": "^2.1.2"
	},
	"packageManager": "pnpm@9.12.0+sha512.4abf725084d7bcbafbd728bfc7bee61f2f791f977fd87542b3579dcb23504d170d46337945e4c66485cd12d588a0c0e570ed9c477e7ccdd8507cf05f3f92eaca"
}
