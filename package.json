{
	"name": "cuponz",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "pnpm run build:client && pnpm run build:server",
		"build:languages": "node scripts/generateSupportedLanguages.cjs",
		"build:client": "vite build --ssrManifest --outDir dist/client",
		"build:server": "vite build --ssr src/entry-server.jsx --outDir dist/server",
		"lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"build-dev": "pnpm run dev:client & pnpm run dev:server",
		"build-dev:client": "vite build --ssrManifest --outDir dist/client --watch",
		"build-dev:server": "vite build --ssr src/entry-server.jsx --outDir dist/server --watch"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.56.2",
		"country-list": "^2.3.0",
		"date-fns": "^4.1.0",
		"framer-motion": "^11.11.1",
		"fuse.js": "^7.0.0",
		"libphonenumber-js": "^1.11.9",
		"prop-types": "^15.8.1",
		"react": "^18.3.1",
		"react-country-flag": "^3.1.0",
		"react-dom": "^18.3.1",
		"react-icons": "^5.3.0",
		"react-qr-code": "^2.0.15",
		"react-router-dom": "^6.26.2",
		"react-select": "^5.8.1",
		"react-toastify": "^10.0.5",
		"sonner": "^1.5.0",
		"validator": "^13.12.0",
		"zustand": "^4.5.5"
	},
	"devDependencies": {
		"@types/react": "^18.3.9",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.1",
		"@vitejs/plugin-react-swc": "^3.7.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^8.57.1",
		"eslint-plugin-react": "^7.36.1",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.12",
		"http-proxy": "^1.18.1",
		"postcss": "^8.4.47",
		"tailwindcss": "^3.4.13",
		"vite": "^5.4.7",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-purge": "^1.0.3",
		"vite-plugin-tailwind-purgecss": "^0.3.3"
	}
}
